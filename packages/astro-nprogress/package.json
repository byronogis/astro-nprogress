{
  "name": "astro-nprogress",
  "type": "module",
  "version": "0.0.0",
  "description": "Nprogress using in astro while the astro view transition is enabled.",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/astro-nprogress#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/astro-nprogress.git"
  },
  "bugs": "https://github.com/byronogis/astro-nprogress/issues",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json",
      "./component": "./src/components/NProgress.astro"
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./component": "./src/components/NProgress.astro"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/components"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepare": "astro sync",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  },
  "dependencies": {
    "astro-view-transition-script": "^0.0.1",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@types/nprogress": "catalog:types",
    "typescript": "catalog:deps"
  }
}
